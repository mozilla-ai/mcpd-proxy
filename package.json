{
  "name": "@mozilla-ai/mcpd-proxy",
  "version": "0.0.1",
  "type": "module",
  "description": "MCP server proxy for mcpd daemon",
  "main": "./dist/index.js",
  "bin": {
    "mcpd-proxy": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "mcpd",
    "model-context-protocol",
    "proxy",
    "ai",
    "llm"
  ],
  "author": "Mozilla AI",
  "license": "Apache-2.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@mozilla-ai/mcpd": "file:../mcpd-sdk-javascript"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.46.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
